logowidth="50"
Red='\033[0m\033[31m'         # Red
White='\033[0m\033[37m'        # White
lRed='\033[0m\033[1;91m'         # Light Red
dGrey='\033[0m\033[1;30m'         # Dark Gray
fulloutput=(
"${lRed}                            *********               "
"${lRed}                       ******************           "
"${lRed}                   ************  ************       "
